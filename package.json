{
  "name": "menu-digital",
  "version": "1.0.0",
  "description": "Sistema de cardápio digital moderno com carrinho de compras e integração WhatsApp",
  "author": "Seu Nome <seuemail@exemplo.com>",
  "license": "MIT",
  "keywords": [
    "cardapio",
    "restaurante",
    "menu",
    "whatsapp",
    "nextjs",
    "ecommerce",
    "food",
    "digital-menu"
  ],
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "analyze": "cross-env ANALYZE=true npm run build",
    "export": "next export",
    "clean": "rm -rf .next out dist",
    "docker:build": "docker build -t menu-digital .",
    "docker:run": "docker run -p 3000:3000 menu-digital",
    "docker:compose": "docker-compose up -d"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "autoprefixer": "^10.4.21",
    "next": "15.4.5",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.3.8",
    "react-image-crop": "^11.0.10",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "typescript": "^5"
  }
}
